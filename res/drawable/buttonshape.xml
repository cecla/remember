<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#0c5d87" />
      <stroke
          android:width="1dp"
          android:color="#0c5d87" />
      <corners
     		android:bottomRightRadius="4dp"
     		android:bottomLeftRadius="4dp"
  			android:topLeftRadius="4dp"
 			android:topRightRadius="4dp"/>
      <padding
          android:left="7dp"
          android:top="7dp"
          android:right="7dp"
          android:bottom="7dp" />
    </shape>
  </item>
  <item>
    <shape>
        <!-- android:startColor="#0c5d87" -->
      <gradient
          android:startColor="#FF33B5E5"
          android:endColor="#FF33B5E5"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#FF33B5E5" />
      <corners
     		android:bottomRightRadius="4dp"
     		android:bottomLeftRadius="4dp"
  			android:topLeftRadius="4dp"
 			android:topRightRadius="4dp"/>
      <padding
          android:left="7dp"
          android:top="7dp"
          android:right="7dp"
          android:bottom="7dp" />
    </shape>
  </item>
</selector>
